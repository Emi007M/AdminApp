<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>

<FlowPane prefHeight="200.0" prefWidth="1000.0" 
          vgap="20.0" hgap="20.0"
          xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" 
          fx:controller="adminapp.presentation.currentTournamentTab.views.ContestantsController">
    <children>
        <HBox>
            <children>
                <Button fx:id="gobackBtn" styleClass="goBackBtn" text="go back" textAlignment="CENTER" wrapText="true" />
                <Label fx:id="titleLabel" text="Contestants Manager" />
   
            </children>
        </HBox>
      <ButtonBar prefHeight="40.0" prefWidth="900.0">
        <buttons>
          <Button fx:id="generateFormBtn" mnemonicParsing="false" onAction="#handleGenerateFormBtn" text="Generate Application Form" />
            <Button fx:id="importFormBtn" mnemonicParsing="false" onAction="#handleImportFormBtn" text="Import Club Application" />
            <Button fx:id="saveAllBtn" mnemonicParsing="false" onAction="#handleSaveChangesBtn" text="Save Changes" />
        </buttons>
      </ButtonBar>
      <ScrollPane prefHeight="350.0" prefWidth="900.0">
         <content>
            <Accordion fx:id="accordion" />
         </content>
      </ScrollPane>
     
    </children>
        
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
</FlowPane>
